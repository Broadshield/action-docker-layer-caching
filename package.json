    "ts-node": "^10.8.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "typescript-eslint-language-service": "^5.0.0"
  },
  "scripts": {
    "test": "tsc --noEmit && NODE_ENV=testing jest --coverage",
    "husky-run": "echo \"${*}\"",
    "test:act": "act -v",
    "current-version": "echo \"${npm_package_version}\"",
    "test-ci": "tsc --noEmit && NODE_ENV=testing jest --ci --reporters=default --reporters=jest-junit",
    "format": "prettier --write src/ .github/ __tests__/",
    "format-check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "lint": "eslint --fix \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "build:save": "yarn exec esbuild src/save.ts --bundle --outdir=dist/save/ --platform=node --target=node16.10 --sourcemap --format=cjs",
    "build:restore": "yarn exec esbuild src/restore.ts --bundle --outdir=dist/restore/ --platform=node --target=node16.10 --sourcemap --format=cjs",
    "build": "yarn build:save && yarn build:restore",
    "build:ncc": "rimraf ./dist;tsc && ncc build -o dist/restore src/restore.ts && ncc build -o dist/save src/save.ts",
    "release": "yarn run format && yarn run lint && yarn run build && git add -f dist",
    "all": "yarn run build && yarn run format && yarn run lint && git add -f dist && yarn test"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "actions",
    "node",
    "cache",
    "docker",
    "containers"
  ],
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "lint-staged": {
    "*.{yml,json}": "prettier --write",
    "*.ts": [
      "prettier --write",
      "eslint --cache --fix",
      "NODE_ENV=testing jest --bail --findRelatedTests --passWithNoTests"
    ],
    "*.js": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged;yarn build;git add dist"
    }
  },
  "packageManager": "yarn@3.2.1",
  "stableVersion": "0.0.11"
}
